{
    "model_name": "ru_bert_parser_with_tagger",
    "embedder": {
      "name": "ru_bert",
      "dropout": 0.4,
      "use_pymorphy": false
    },
    "encoder": {
      "encoder_type": "lstm",
      "lstm_dim": 256,
      "lstm_num_layers": 1,
      "lstm_dropout": 0.3
    },
    "parser": {
      "dropout": 0.1,
      "tag_representation_dim": 128,
      "arc_representation_dim": 512
    },
    "trainer": {
      "batch_size": 32,
      "num_epochs": 7,
      "patience": 5,
      "num_gradient_accumulation_steps": 1
    },
    "data": {
      "data_dir": "../data",
      "pretrained_models_dir": "../pretrained_models",
      "models_dir": "../models",
      "train_data_all_except": null,
      "train_data": ["GramEval2020-GSD-train.conllu"],
      "valid_data": ["GramEval2020-GSD-wiki-dev.conllu"]
    }
  }